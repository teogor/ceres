    <enum name="onErrorContainer" value="25" />
    <enum name="outline" value="26" />
    <enum name="outlineVariant" value="27" />
    <enum name="scrim" value="28" />

    <enum name="onPrimaryMenu" value="29" />
  </attr>
  <attr name="surface_tint_overlay" format="enum">
    <enum name="primary" value="0" />
    <enum name="onPrimary" value="1" />
    <enum name="primaryContainer" value="2" />
    <enum name="onPrimaryContainer" value="3" />
    <enum name="inversePrimary" value="4" />
    <enum name="secondary" value="5" />
    <enum name="onSecondary" value="6" />
    <enum name="secondaryContainer" value="7" />
    <enum name="onSecondaryContainer" value="8" />
    <enum name="tertiary" value="9" />
    <enum name="onTertiary" value="10" />
    <enum name="tertiaryContainer" value="11" />
    <enum name="onTertiaryContainer" value="12" />
    <enum name="background" value="13" />
    <enum name="onBackground" value="14" />
    <enum name="surface" value="15" />
    <enum name="onSurface" value="16" />
    <enum name="surfaceVariant" value="17" />
    <enum name="onSurfaceVariant" value="18" />
    <enum name="surfaceTint" value="19" />
    <enum name="inverseSurface" value="20" />
    <enum name="inverseOnSurface" value="21" />
    <enum name="error" value="22" />
    <enum name="onError" value="23" />
    <enum name="errorContainer" value="24" />
    <enum name="onErrorContainer" value="25" />
