<resources>
  <!-- Theme to apply to the Activity once the splash screen is dismissed-->
  <attr name="afterSplashScreenTheme" format="reference" />

  <!-- Flag that determines whether elevation overlays (translucent overlay based on
       elevation to provide contrast) are enabled. Default is false in light themes
       and true in dark themes. -->
  <attr name="elevationOverlayEnabled" format="boolean" />
  <!-- The color of the elevation overlay layer, which will be used with an alpha
       percentage based on elevation. -->
  <attr name="elevationOverlayColor" format="color" />
  <!-- The color of the elevation overlay accent layer, which will be placed
       on top of the composited background and overlay color. -->
  <attr name="elevationOverlayAccentColor" format="color" />

  <!-- The color of surfaces such as cards, sheets, menus. -->
  <attr name="colorSurface" format="color" />

  <attr name="consumeTop" format="boolean" />

  <attr name="seekBarStyle" format="reference" />

  <attr name="surface_level" format="enum">
    <enum name="transparent" value="0" />
    <enum name="lvl_0" value="1" />
    <enum name="lvl_1" value="2" />
    <enum name="lvl_2" value="3" />
    <enum name="lvl_3" value="4" />
    <enum name="lvl_4" value="5" />
    <enum name="lvl_5" value="6" />
    <enum name="lvl_6" value="7" />
    <enum name="lvl_7" value="8" />
    <enum name="lvl_8" value="9" />
  </attr>

  <attr name="color_m3" format="enum">
    <enum name="primary" value="0" />
    <enum name="onPrimary" value="1" />
    <enum name="primaryContainer" value="2" />
    <enum name="onPrimaryContainer" value="3" />
    <enum name="inversePrimary" value="4" />
    <enum name="secondary" value="5" />
    <enum name="onSecondary" value="6" />
    <enum name="secondaryContainer" value="7" />
    <enum name="onSecondaryContainer" value="8" />
    <enum name="tertiary" value="9" />
    <enum name="onTertiary" value="10" />
    <enum name="tertiaryContainer" value="11" />
    <enum name="onTertiaryContainer" value="12" />
    <enum name="background" value="13" />
    <enum name="onBackground" value="14" />
    <enum name="surface" value="15" />
    <enum name="onSurface" value="16" />
    <enum name="surfaceVariant" value="17" />
    <enum name="onSurfaceVariant" value="18" />
    <enum name="surfaceTint" value="19" />
    <enum name="inverseSurface" value="20" />
    <enum name="inverseOnSurface" value="21" />
    <enum name="error" value="22" />
    <enum name="onError" value="23" />
    <enum name="errorContainer" value="24" />
    <enum name="onErrorContainer" value="25" />
    <enum name="outline" value="26" />
    <enum name="outlineVariant" value="27" />
    <enum name="scrim" value="28" />

    <enum name="onPrimaryMenu" value="29" />
  </attr>
  <attr name="background_m3" format="enum">
    <enum name="primary" value="0" />
    <enum name="onPrimary" value="1" />
    <enum name="primaryContainer" value="2" />
    <enum name="onPrimaryContainer" value="3" />
    <enum name="inversePrimary" value="4" />
    <enum name="secondary" value="5" />
    <enum name="onSecondary" value="6" />
    <enum name="secondaryContainer" value="7" />
    <enum name="onSecondaryContainer" value="8" />
    <enum name="tertiary" value="9" />
    <enum name="onTertiary" value="10" />
    <enum name="tertiaryContainer" value="11" />
    <enum name="onTertiaryContainer" value="12" />
    <enum name="background" value="13" />
    <enum name="onBackground" value="14" />
    <enum name="surface" value="15" />
    <enum name="onSurface" value="16" />
    <enum name="surfaceVariant" value="17" />
    <enum name="onSurfaceVariant" value="18" />
    <enum name="surfaceTint" value="19" />
    <enum name="inverseSurface" value="20" />
    <enum name="inverseOnSurface" value="21" />
    <enum name="error" value="22" />
    <enum name="onError" value="23" />
    <enum name="errorContainer" value="24" />
    <enum name="onErrorContainer" value="25" />
    <enum name="outline" value="26" />
    <enum name="outlineVariant" value="27" />
    <enum name="scrim" value="28" />

    <enum name="onPrimaryMenu" value="29" />
  </attr>
  <attr name="text_color_m3" format="enum">
    <enum name="primary" value="0" />
    <enum name="onPrimary" value="1" />
    <enum name="primaryContainer" value="2" />
    <enum name="onPrimaryContainer" value="3" />
    <enum name="inversePrimary" value="4" />
    <enum name="secondary" value="5" />
    <enum name="onSecondary" value="6" />
    <enum name="secondaryContainer" value="7" />
    <enum name="onSecondaryContainer" value="8" />
    <enum name="tertiary" value="9" />
    <enum name="onTertiary" value="10" />
    <enum name="tertiaryContainer" value="11" />
    <enum name="onTertiaryContainer" value="12" />
    <enum name="background" value="13" />
    <enum name="onBackground" value="14" />
    <enum name="surface" value="15" />
    <enum name="onSurface" value="16" />
    <enum name="surfaceVariant" value="17" />
    <enum name="onSurfaceVariant" value="18" />
