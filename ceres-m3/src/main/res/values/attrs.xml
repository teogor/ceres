  <attr name="ripple_enabled" format="boolean" />
  <attr name="title" format="string|reference" />
  <attr name="subtitle" format="string|reference" />
  <attr name="has_switch" format="boolean" />
  <attr name="initial_callback" format="boolean" />
  <attr name="checked" format="boolean" />
  <attr name="pickedColor" format="reference|color" />
  <attr name="corner_radius" format="dimension" />
  <attr name="src_image" format="reference" />
  <attr name="padding_image" format="reference|dimension" />
  <attr name="surface_tint" format="float" />

  <declare-styleable name="Bar">
    <attr name="consumeTop" />
  </declare-styleable>

  <declare-styleable name="BarAnimator">
    <attr name="constraint_referenced_ids" />
  </declare-styleable>

  <declare-styleable name="FragmentContainer">
    <attr name="consumeTop" />
    <attr name="android:paddingTop" />
  </declare-styleable>

  <declare-styleable name="ContainerBaseM3">
    <attr name="background_m3" />
    <attr name="surface_tint" />
    <attr name="surface_tint_overlay" />
    <attr name="corner_radius" />
    <attr name="surface_level" />
    <attr name="ripple_enabled" />
  </declare-styleable>

  <declare-styleable name="ButtonM3">
    <attr name="background_m3" />
    <attr name="text_color_m3" />
    <attr name="corner_radius" />
    <attr name="surface_level" />
    <attr name="ripple_enabled" />
  </declare-styleable>

  <declare-styleable name="SwitchComponentM3">
    <attr name="android:fontFamily" />
    <attr name="title" />
    <attr name="subtitle" />
    <attr name="has_switch" />
    <attr name="initial_callback" />
    <attr name="checked" />
  </declare-styleable>

  <declare-styleable name="ImageComponentM3">
    <attr name="android:fontFamily" />
    <attr name="title" />
    <attr name="subtitle" />
    <attr name="src_image" />
    <attr name="padding_image" />
  </declare-styleable>

  <declare-styleable name="SnackbarM3">
    <attr name="android:fontFamily" />
    <attr name="title" />
  </declare-styleable>

  <declare-styleable name="ColorsContainerM3">
    <attr name="android:fontFamily" />
    <attr name="title" />
    <attr name="pickedColor" />
  </declare-styleable>

  <declare-styleable name="ImageViewM3">
    <attr name="surface_level" />
    <attr name="ripple_enabled" />
    <attr name="color_m3" />
  </declare-styleable>

  <declare-styleable name="TextViewM3">
    <attr name="surface_level" />
    <attr name="color_m3" />
    <attr name="ripple_enabled" />
    <attr name="corner_radius" />
  </declare-styleable>

  <declare-styleable name="RatingBarM3">
    <attr name="color_m3" />
  </declare-styleable>

  <declare-styleable name="DigitsImageM3">
    <attr name="shape" />
    <attr name="background_m3" />
    <attr name="text_color_m3" />
    <attr name="corner_radius" />
    <attr name="surface_level" />
    <attr name="ripple_enabled" />
  </declare-styleable>

  <declare-styleable name="HSLColorPickerSeekBar">
    <attr name="hslMode" format="enum">
      <enum name="hue" value="0" />
      <enum name="saturation" value="1" />
      <enum name="lightness" value="2" />
    </attr>
    <attr name="hslColoringMode" format="enum">
      <enum name="pure" value="0" />
      <enum name="output" value="1" />
    </attr>
  </declare-styleable>

  <declare-styleable name="CMYKColorPickerSeekBar">
    <attr name="cmykMode" format="enum">
      <enum name="cyan" value="0" />
      <enum name="magenta" value="1" />
      <enum name="yellow" value="2" />
      <enum name="black" value="3" />
    </attr>
    <attr name="cmykColoringMode" format="enum">
      <enum name="pure" value="0" />
      <enum name="output" value="1" />
    </attr>
  </declare-styleable>

  <declare-styleable name="RGBColorPickerSeekBar">
    <attr name="rgbMode" format="enum">
      <enum name="red" value="0" />
      <enum name="green" value="1" />
      <enum name="blue" value="2" />
    </attr>
    <attr name="rgbColoringMode" format="enum">
      <enum name="pure" value="0" />
      <enum name="output" value="1" />
      <enum name="plain" value="2" />
    </attr>
  </declare-styleable>

  <declare-styleable name="LABColorPickerSeekBar">
    <attr name="labMode" format="enum">
      <enum name="l" value="0" />
      <enum name="a" value="1" />
      <enum name="b" value="2" />
    </attr>
    <attr name="labColoringMode" format="enum">
      <enum name="output" value="0" />
    </attr>
  </declare-styleable>
</resources>
